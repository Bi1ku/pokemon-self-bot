{
	"name": "pwitter",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "node ./src/app.js",
		"watch": "nodemon ./src/app.js",
		"run-bot": "python3 main.py"
	},
	"dependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.8.7",
		"@babel/preset-env": "^7.8.7",
		"@prisma/client": "^4.3.1",
		"aws-sdk": "^2.809.0",
		"babel-core": "7.0.0-bridge.0",
		"babel-loader": "^8.0.0-beta.6",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"express": "~4.16.1",
		"http-errors": "~1.6.3",
		"jsonwebtoken": "^8.5.1",
		"morgan": "~1.9.1",
		"multer": "^1.4.2",
		"multer-s3": "^2.9.0",
		"nodemon": "^2.0.2",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"cross-env": "^7.0.3",
		"prisma": "^4.3.1"
	},
	"nodemonConfig": {
		"exec": "npm run dev",
		"watch": [
			"server/*",
			"public/*"
		],
		"ignore": [
			"**/__tests__/**",
			"*.test.js",
			"*.spec.js"
		]
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"esmodules": true
					}
				}
			]
		]
	}
}
